{
  "name": "battlesnake",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  },
  "scripts": {
    "dev:build": "tsc --preserveWatchOutput --project config/tsconfig.json",
    "dev:build:watch": "npm run dev:build --watch",
    "dev:run": "nodemon build/index.js",
    "dev": "concurrently \"npm run dev:build:watch\" \"npm run dev:run\"",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmusey/battlesnake.git"
  },
